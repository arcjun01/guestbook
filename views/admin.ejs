<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin - Guestbook Entries</title>
    <link rel="stylesheet" href="/styles/home.css">
</head>
<body>
    <h1>Guestbook Entries</h1>
    <table border="1">
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Job Title</th>
                <th>Company</th>
                <th>LinkedIn</th>
                <th>Email</th>
                <th>How We Met</th>
                <th>Other</th>
                <th>Message</th>
                <th>Mailing List</th>
                <th>Format</th>
                <th>Created At</th>
            </tr>
        </thead>
        <tbody>
            <% guestbook.forEach(entry => { %>
            <tr>
                <td><%= entry.first_name %></td>
                <td><%= entry.last_name %></td>
                <td><%= entry.job_title %></td>
                <td><%= entry.company %></td>
                <td><%= entry.linkedin %></td>
                <td><%= entry.email %></td>
                <td><%= entry.meet %></td>
                <td><%= entry.other %></td>
                <td><%= entry.message %></td>
                <td><%= entry.mailingList %></td>
                <td><%= entry.format %></td>
                <td><%= entry.created %></td>
            </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
